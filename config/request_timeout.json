{
    "name": "gogo",
    "mode": "test",
    "sections": {
        "development": {
            "server": {
                "addr": "localhost",
                "port": 9090,
                "request_timeout": 1,
                "ssl": false
            },
            "logger": {
                "output": "stdout",
                "level": "debug"
            },
            "app": {
                "access_key": "KEY",
                "access_secret": "SECRET"
            }
        },
        "test": {
            "server": {
                "addr": "localhost",
                "port": 9090,
                "ssl": false
            },
            "logger": {
                "output": "stdout",
                "level": "info"
            }
        },
        "production": {
            "server": {
                "addr": "localhost",
                "port": 9090,
                "ssl": true,
                "ssl_cert": "/path/to/ssl/cert",
                "ssl_key": "/path/to/ssl/key"
            },
            "logger": {
                "output": "stdout",
                "level": "warn"
            }
        }
    }
}
